<%= form_tag(search_results_path, method: "get") do %>
      <%= label_tag(:q, "Search for:") %>
      <%= text_field_tag(:q) %>
      <%= submit_tag("Search") %>
    <%end%>
<div class="menu">
  <h1>Connor Soccer Shop</h1>
  <nav>
